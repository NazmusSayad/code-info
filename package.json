{
  "name": "@nazmussayad/code-info",
  "version": "0.0.0",
  "description": "Get information about your source code.",
  "main": "./dist/mjs/index.js",
  "types": "./dist/mjs/index.d.js",
  "bin": {
    "ci": "node ./dist/cjs/bin/index.js",
    "code-info": "node ./dist/cjs/bin/index.js"
  },
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/mjs/index.js"
    }
  },
  "scripts": {
    "ci": "node --watch ./dist/cjs/bin/index.js",
    "cjs": "node --watch test/index.cjs",
    "mjs": "node --watch test/index.mjs",
    "dev": "node dist.js && tsc -p tsconfig-cjs.json -w",
    "build": "node build.js",
    "update": "node update.js"
  },
  "dependencies": {
    "node-ls-files": "^0.0.3"
  },
  "devDependencies": {
    "@types/node": "latest",
    "typescript": "latest"
  },
  "type": "module",
  "license": "ISC",
  "author": "https://github.com/NazmusSayad",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NazmusSayad/code-info.git"
  },
  "bugs": {
    "url": "https://github.com/NazmusSayad/code-info/issues"
  },
  "homepage": "https://github.com/NazmusSayad/code-info#readme"
}
